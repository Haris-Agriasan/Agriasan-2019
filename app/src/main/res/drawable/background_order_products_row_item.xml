<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">

        <shape android:shape="rectangle">
            <gradient android:angle="225" android:endColor="#E6DC98" android:startColor="#E6DC98" />
            <corners android:radius="7dp" />
        </shape>

    </item>

    <item>
        <shape android:shape="rectangle">
            <gradient android:angle="225" android:endColor="#FFFF" android:startColor="#FFFF" />
            <corners android:radius="7dp" />
            <stroke
                android:width="2dp"
                android:color="#000"
                />
        </shape>

    </item>
</selector>
