<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">

    <shape android:shape="rectangle">
        <gradient android:angle="225" android:endColor="#E6DC98" android:startColor="#E6DC98" />
        <corners android:radius="10dp" />
    </shape>

</item>
<item>
    <shape android:shape="rectangle">
        <gradient android:angle="225" android:endColor="#73BF43" android:startColor="#73BF43" />
        <corners android:radius="10dp" />
    </shape>

</item>
</selector>

